{
  "name": "seed-test-backend",
  "version": "1.8.0",
  "engines": {
    "node": "20.12.1"
  },
  "scripts": {
    "start": "tsc && node src/server.js",
    "test": "jest --verbose",
    "test-coverage": "jest --coverage",
    "database": "node src/database/installDatabase.js",
    "database-examples": "node src/database/insertExampleData.js",
    "clean": "rimraf ./dist",
    "tsc": "tsc",
    "lint": "./node_modules/.bin/eslint src/database --fix",
    "exec-test": "./node_modules/.bin/cucumber-js"
  },
  "private": true,
  "dependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "adm-zip": "^0.5.16",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "chai": "^4.5.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cucumber-html-reporter": "^7.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-flash": "0.0.2",
    "express-session": "^1.18.1",
    "geckodriver": "^5.0.0",
    "moment": "^2.30.1",
    "mongodb": "^6.15.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "selenium-webdriver": "^4.27.0",
    "stream-to-string": "^1.2.1",
    "string-to-stream": "^3.0.1",
    "tar": "^7.4.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.9",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "typescript": ">=5.4.0 <5.6.0"
  },
  "_comment": "Bitte bei cucumber-html-reporter > 7.1.1 wieder entfernen!",
  "overrides": { 
    "cucumber-html-reporter": {
      "semver": "^7.5.2"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/spec/server.spec.js",
      "<rootDir>/spec/serverHelper.spec.js",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "./spec/setupTest.js"
    ]
  }
}
