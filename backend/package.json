{
  "name": "seed-test-backend",
  "version": "1.7.2",
  "engines": {
    "node": "^18.13.0 || ^20.9.0"
  },
  "scripts": {
    "start": "tsc && node src/server.js",
    "test": "jest --verbose",
    "test-coverage": "jest --coverage",
    "database": "node src/database/installDatabase.js",
    "database-examples": "node src/database/insertExampleData.js",
    "clean": "rimraf ./dist",
    "tsc": "tsc",
    "lint": "./node_modules/.bin/eslint src/database --fix",
    "exec-test": "./node_modules/.bin/cucumber-js"
  },
  "private": true,
  "dependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "adm-zip": "^0.5.14",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chai": "^4.4.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cucumber-html-reporter": "^7.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-flash": "0.0.2",
    "express-session": "^1.18.0",
    "geckodriver": "^4.4.1",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "selenium-webdriver": "^4.22.0",
    "stream-to-string": "^1.2.1",
    "string-to-stream": "^3.0.1",
    "tar": "^7.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "typescript": ">=5.2.0 <5.5.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/spec/server.spec.js",
      "<rootDir>/spec/serverHelper.spec.js",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "./spec/setupTest.js"
    ]
  }
}
